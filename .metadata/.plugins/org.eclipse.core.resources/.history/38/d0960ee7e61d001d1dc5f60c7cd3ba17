package java1;

public class p3_operator {
	
	//001 Toan tu so hoc + - * / %
	public static void main(String[] args) {
		int numberOne = 20;
		int numberTwo = 3;
		int result;
		
		// +
		result = numberOne + numberTwo;
		System.out.println(numberOne + " + " + numberTwo + " = " + result);
		
		// -
		result = numberOne - numberTwo;
		System.out.println(numberOne + " - " + numberTwo + " = " + result);
		
		// *
		result = numberOne * numberTwo;
		System.out.println(numberOne + " * " + numberTwo + " = " + result);
		
		// /
		result = numberOne / numberTwo;
		System.out.println(numberOne + " / " + numberTwo + " = " + result);
		
		// %
		result = numberOne % numberTwo;
		System.out.println(numberOne + " - " + numberTwo + " = " + result);
	}
}
