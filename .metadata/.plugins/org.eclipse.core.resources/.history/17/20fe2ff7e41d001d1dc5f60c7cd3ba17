package java1;

public class b4 {
	public static void main(String[] args) {
		int var1 = 12;
		byte var2 = 10;
		boolean var3 = true;
		double var4 = 123.456;
		
		System.out.println("var1: " + var1);
		System.out.println("var2: " + var2);
		System.out.println("var3: " + var3);
		System.out.println("var4: " + var4);
		
	}
}
